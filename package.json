{
  "name": "kintone-dify-chat-embed-plugin",
  "description": "kintone plugin to embed Dify chat and toggle keyboard shortcuts",
  "version": "0.1.0",
  "scripts": {
    "start": "node scripts/npm-start.js",
    "upload": "cli-kintone plugin upload --input dist/plugin.zip --watch",
    "develop": "npm run build -- --watch",
    "keygen": "cli-kintone plugin keygen --output private.ppk",
    "build": "cli-kintone plugin pack --private-key private.ppk --output dist/plugin.zip --input src/manifest.json",
    "lint": "eslint ."
  },
  "keywords": [
    "kintone",
    "dify",
    "chat",
    "embed",
    "keyboard shortcuts",
    "productivity"
  ],
  "author": {
    "name": "r3-yamauchi",
    "url": "https://yamauchi.win"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^24.3.0",
    "@kintone/cli": "^1.15.0",
    "npm-run-all": "^4.1.5",
    "eslint": "^9.39.1"
  }
}